#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@hail2u/css-mqpacker/bin/mqpacker.js" "$@"
else 
  exec node  "$basedir/../@hail2u/css-mqpacker/bin/mqpacker.js" "$@"
fi
